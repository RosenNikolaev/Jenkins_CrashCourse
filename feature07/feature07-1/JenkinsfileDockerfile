pipeline {
    agent none 
    stages {
        stage ('Custom Dockerfile name'){
            agent {
              dockerfile {
                filename '/feature07/feature07-1/Dockerfile.custom'
              }
            }
            steps{ 
              echo $MY_ENV
            }
        }
    }
  post {
    always {
      node ('built-in'){
        sh 'docker ps'
        sh 'docker image ls'
        sh 'docker volume ls'
      }

    }
  }
}