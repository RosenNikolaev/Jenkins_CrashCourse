pipeline {
    agent none 
    stages {
        stage('Custom Dockerfile') {
            agent {
                dockerfile true 
            }
            steps {
              echo $MY_ENV
            }
        }
        stage ('Custom Dockerfile name'){
            agent {
              dockerfile {
                filename 'Dockerfile.custom'
              }
            }
            steps{ 
              echo $MY_ENV
            }
        }
    }
  post {
    always {
      node ('built-int'){
        sh 'docker ps'
        sh 'docker image ls'
        sh 'docker volume ls'
      }

    }
  }
}